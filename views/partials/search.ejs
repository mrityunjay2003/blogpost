<div class="searchBar">
  <div class="container">
    <form
      action="/search-results"
      class="search__form"
      role="search"
      method="POST"
    >
      <input
        type="search"
        aria-label="Search"
        id="searchInput"
        name="SearchTerm"
        placeholder="Search the site."
      />
    </form>
    <div id="searchClose"><img src="/img/close.png" alt="close" /></div>
  </div>
</div>
